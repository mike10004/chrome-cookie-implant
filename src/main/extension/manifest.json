{
  "manifest_version": 2,

  "name": "Cookie Implant",
  "description": "Add cookies to your Chrome profile",
  "version": "1.5",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Implant Cookies"
  },

  "permissions": [
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  
  "homepage_url": "https://github.com/mike10004/chrome-cookie-implant",
  
  "options_page": "options.html",
  
  "minimum_chrome_version": "18"
}

